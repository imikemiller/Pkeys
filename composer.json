{
    "name": "imikemiller/pkeys",
    "description": "An key management library to keep your key strings consistent, to avoid duplication and to stop the crime of inlining keys in your code",
    "type": "library",
    "require": {
        "phpunit/phpunit": "^6.3@dev",
        "illuminate/support": "^5.5@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Miller",
            "email": "mike@mikemiller.uk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Pkeys\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
